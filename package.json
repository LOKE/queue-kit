{
  "name": "@loke/queue-kit",
  "version": "2.3.0",
  "description": "A lib for rabbit and sqs queues",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "eslint . && ava -v",
    "lint": "eslint . ",
    "prepare": "npm run build",
    "build": "rm -rf ./dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LOKE/queue-kit.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "default": "./dist/common.js"
    },
    "./rabbit": {
      "types": "./dist/rabbit.d.ts",
      "default": "./dist/rabbit.js"
    },
    "./sqs": {
      "types": "./dist/sqs.d.ts",
      "default": "./dist/sqs.js"
    }
  },
  "author": "Dominic Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LOKE/queue-kit/issues"
  },
  "homepage": "https://github.com/LOKE/queue-kit#readme",
  "dependencies": {
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.637.0",
    "@types/amqplib": "^0.10.5",
    "@types/node": "^20.16.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "amqplib": "^0.10.4",
    "ava": "^6.1.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.8",
    "prom-client": "^14.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
